<body>
    <div class="wrapper">
        <!-- Sidebar -->
        <nav id="sidebar">
            <div class="sidebar-header">
                <h3>Table # <label id='tblNumber'></label></h3>
            </div>

            <ul class="list-unstyled components">
                <li class="active">
                    <a href="#" onclick="hideContent('content')">Table View</a>
                </li>
                <li>
                    <a href="#" onclick="hideContent('orderinfo')">Order Info</a>
                </li>
                <li>
                    <a href="#" onclick="hideContent('service')">Service Request</a>
                </li>
            </ul>
        </nav>

        <!-- tables -->
        <div id="content">
            <!-- Table Grid -->
            <!-- First ROW -->
            <div class="table-layout">
                <div class="box" id="box1" class="container-fluid">
                    <button class="boxButton" type="button">
                        <span> 1 <span>
                    </button>
                </div>
                <div class="box" id="box2" class="container-fluid">
                    <button class="boxButton" type="button">
                        <span> 2 <span>
                    </button>
                </div>
                <div class="box" id="box3" class="container-fluid">
                    <button class="boxButton" type="button">
                        <span> 3 <span>
                    </button>
                </div>
                <div class="box" id="box4" class="container-fluid">
                    <button class="boxButton" type="button">
                        <span> 4 <span>
                    </button>
                </div>
                <div class="box" id="box5" class="container-fluid">
                    <button class="boxButton" type="button">
                        <span> 5 <span>
                    </button>
                </div>
                <div class="box" id="box6" class="container-fluid">
                    <button class="boxButton" type="button">
                        <span> 6 <span>
                    </button>
                </div>
                <div class="box" id="box7" class="container-fluid">
                    <button class="boxButton" type="button">
                        <span> 7 <span>
                    </button>
                </div>
                <div class="box" id="box8" class="container-fluid">
                    <button class="boxButton" type="button">
                        <span> 8 <span>
                    </button>
                </div>
            </div>

            <div class="table-layout">
                <div class="box" id="box9" class="container-fluid">
                    <button class="boxButton" type="button">
                        <span> 9 <span>
                    </button>
                </div>
                <div class="box" id="box10" class="container-fluid">
                    <button class="boxButton" type="button">
                        <span> 10 <span>
                    </button>
                </div>
                <div class="box" id="box11" class="container-fluid">
                    <button class="boxButton" type="button">
                        <span> 11 <span>
                    </button>
                </div>
                <div class="box" id="box12" class="container-fluid">
                    <button class="boxButton" type="button">
                        <span> 12 <span>
                    </button>
                </div>
                <div class="box" id="box13" class="container-fluid">
                    <button class="boxButton" type="button">
                        <span> 13 <span>
                    </button>
                </div>
                <div class="box" id="box14" class="container-fluid">
                    <button class="boxButton" type="button">
                        <span> 14 <span>
                    </button>
                </div>
                <div class="box" id="box15" class="container-fluid">
                    <button class="boxButton" type="button">
